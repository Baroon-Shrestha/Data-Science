   "source": [
    "import math\n",
    "\n",
    "def standard_Deviation(lst):\n",
    "    mean = sum(lst) / len(lst)\n",
    "    variance = sum((n-mean)**2 for n in lst) / len(lst)\n",
    "\n",
    "    return math.sqrt(variance)\n",
    "    \n",
    "standard_Deviation((1,2,3,4,5))"
   ]
